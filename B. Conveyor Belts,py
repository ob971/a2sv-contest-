def solve():
    n = int(input())
    num = list(map(int, input().split()))
    freq = {}
    for i in range(1, n + 1):
        if num[i] in freq:
            freq[num[i]] += 1
        else:
            freq[num[i]] = 1

    for j in freq.items():
        if j[1] == 1:
            print("-1")
            return

    tot = 0
    i = 1
    for j in freq.items():
        print(j[1] + tot, end=" ")
        while i < j[1] + tot:
            print(i, end=" ")
            i += 1
        tot += j[1]
        i = tot + 1

    print()

t = int(input())
for _ in range(t):
    solve()
